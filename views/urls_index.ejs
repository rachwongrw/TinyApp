<header>
    <% include ./partials/_header %>
</header>
<body>
    <h2>List of URLs and their shortened forms</h2>

    <ul>
        <% for (shortURL in urls) { %>
            <li><a href='/urls/<%= shortURL %>'><%= shortURL %></a> - <%= urls[shortURL]%></li>
        <% }; %>
    </ul>

    <a href='#'>Some Link</a> 
